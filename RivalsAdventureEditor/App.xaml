<Application x:Class="RivalsAdventureEditor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RivalsAdventureEditor"
             DispatcherUnhandledException="App_DispatcherUnhandledException"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="EmptyImage" UriSource="pack://application:,,,/Resources/icon4.png"/>
        <BitmapImage x:Key="TargetSprite" UriSource="pack://application:,,,/Resources/article10_0.png"/>
        <BitmapImage x:Key="ProjectIcon" UriSource="pack://application:,,,/Resources/ProjectIcon.png"/>
        <BitmapImage x:Key="WhitePixel" UriSource="pack://application:,,,/Resources/whitepixel.png"/>
        <BitmapImage x:Key="PathPoint" UriSource="pack://application:,,,/Resources/pathpoint.png"/>
        <BitmapImage x:Key="Arrows" UriSource="pack://application:,,,/Resources/arrows.png"/>
        <BitmapImage x:Key="Square" UriSource="pack://application:,,,/Resources/square.png"/>
        <BitmapImage x:Key="Respawn" UriSource="pack://application:,,,/Resources/respawn.png"/>
    </Application.Resources>
</Application>

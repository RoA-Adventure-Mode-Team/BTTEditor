<Grid   x:Class="RivalsAdventureEditor.Panels.RoomEditor"
        xmlns:draw="clr-namespace:RivalsAdventureEditor.DrawingObjects"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:RivalsAdventureEditor.Panels"
        Loaded="OnLoaded"
        MouseDown="OnMouseDown"
        MouseMove="OnMouseMove"
        MouseUp="OnMouseUp"
        MouseWheel="OnMouseWheel"
        KeyDown="OnKeyDown"
        LayoutUpdated="OnLayoutUpdate"
        mc:Ignorable="d" 
        xmlns:cmd="clr-namespace:RivalsAdventureEditor.Commands"
        xmlns:i="clr-namespace:System.Windows.Interop;assembly=PresentationCore"
        x:Name="Self"
        d:DesignHeight="450" d:DesignWidth="800">
    <Image x:Name="imgelt">
        <Image.Source>
            <i:D3DImage x:Name="d3dimg"/>
        </Image.Source>
    </Image>
</Grid>
